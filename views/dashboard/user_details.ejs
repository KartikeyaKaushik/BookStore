<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>dashboard</title>
    <link rel="stylesheet" href="/css/user_details.css" type="text/css">

</head>
<body class="font">
    <!-- #################### header section ###################### -->
    <%- include('dash_header.ejs') %>
    <!-- ##################### header section ends ################## -->

    
    <div class="container-fluid">
        <div class="row">
            <!-- ################## left section #################### -->
            <div class="col-md-3 p-0 heightt bg-warning responsive">
                <%- include('left.ejs') %>
                
            </div>
            <!-- ################## left section ends #################### -->
            <div class="col-md-9 table-responsive-sm offset-md-3">
                <table class="table">
                    <tr>
                        <td>Full Name :</td>
                        <td><%= data.first_name +" "+ data.last_name %></td>
                    </tr>
                    <tr>
                        <td>Username :</td>
                        <td><%=data.username %></td>
                    </tr>
                    <tr>
                        <td>Email :</td>
                        <td><%=data.email %></td>
                    </tr>
                    <tr>
                        <td>Address :</td>
                        <td><%=data.address %></td>
                    </tr>
                    <tr>
                        <td>Password :</td>
                        <td><%=data.password %></td>
                    </tr>
                    <tr>
                        <td>C_Password :</td>
                        <td><%=data.confirm_password %></td>
                    </tr>
                </table>
            </div>
        </div>
    </div>
    <!-- ######################### footer section ###################### -->
    
    <!-- ####################### footer section ends ####################### -->
    
</body>
</html>